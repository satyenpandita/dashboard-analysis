[program:celeryd]
command=<%= current_path %>/env/bin/celery worker -A config  --loglevel=INFO
user=ubuntu
directory=<%= current_path %>
autostart=true
autorestart=true
stopwaitsecs = 600
killasgroup=true
startsecs=10
stdout_logfile=/var/log/celery/aim-celeryd.log
stderr_logfile=/var/log/celery/aim-celeryd.log

[program:gunicorn]
command=<%= current_path %>/env/bin/gunicorn app:app --bind 127.0.0.1:8000 --workers 4 --pid /tmp/gunicorn.pid ;
directory=<%= current_path %>

[program:flower]
command=<%= current_path %>/env/bin/flower -A config --port=5555 ;
directory=<%= current_path %>